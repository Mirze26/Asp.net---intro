
<h1>Home Page</h1>



@{
    int count = 100;

    List<string> names = new List<string>() { "Shaiq", "Ceyhun", "Anar" }; 
}

<section id="home">
    <div class="navbar">
        <h3 style="color:red">Started Backend</h3>

        @{
            var number = 5 + 5;
        }

        <span>@number</span>

        <ul>

            @for (int i = 0; i <= 20; i++)
            {
                <li>@count</li>
                count++;
            }
        </ul>


        @foreach(var item in names)
        {
            <h1>@item</h1>
        }
    </div>


   
</section>



